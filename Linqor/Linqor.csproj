<Project Sdk="Microsoft.NET.Sdk">
    <PropertyGroup>
        <Version>3.0.0</Version>
        <Title>Linqor</Title>
        <Authors>Andrew Morozov</Authors>
        <Copyright>Andrew Morozov</Copyright>
        <Description>Linq extensions for ordered sequences. Provides better perfomance in comparison to standard extensions. Provides Concat, Distinct, Except, GroupBy, Intersect, Join, GroupJoin and Union.</Description>

        <PackageIconUrl>http://i.imgur.com/r0DNvtB.png</PackageIconUrl>
        <PackageProjectUrl>https://github.com/dangerozov/linqor</PackageProjectUrl>
        <PackageTags>linq;ordered;ienumerable;extensions</PackageTags>
        <PackageRequireLicenseAcceptance>false</PackageRequireLicenseAcceptance>
        <PackageLicenseUrl>https://raw.githubusercontent.com/dangerozov/linqor/master/LICENSE</PackageLicenseUrl>
        <PackageReleaseNotes>Except, Intersect and Union are no longer Set specific.</PackageReleaseNotes>

        <RepositoryType>git</RepositoryType>
        <RepositoryUrl>https://github.com/dangerozov/linqor</RepositoryUrl>

        <LangVersion>9.0</LangVersion>
        <TargetFramework>netstandard1.0</TargetFramework>
    </PropertyGroup>
    <ItemGroup>
        <AssemblyAttribute Include="System.Runtime.CompilerServices.InternalsVisibleToAttribute">
            <_Parameter1>Linqor.Tests</_Parameter1>
        </AssemblyAttribute>
    </ItemGroup>
    <ItemGroup>
      <PackageReference Include="System.ValueTuple" Version="4.5.0" />
    </ItemGroup>
</Project>